<% form_for @sprint do |f| %>
  <%= f.error_messages %>

  <%= f.hidden_field :project_id %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :start_date %><br />
    <%= f.date_select :start_date %>
  </p>
  <p>
    <%= f.label :end_date %><br />
    <%= f.date_select :end_date %>
  </p>

  <p>
    <%= f.label :jazz_id %><br />
    <%= f.text_field :jazz_id %>
  </p>


  <p><%= f.submit %></p>
<% end %>
